<div class="courses-display">
  <% courses.forEach((course) => { %>
    <hr>
    <p>Course ID: <%=course.course_id %></p>
  <p>Course Name: <%=course.course_name %></p>
  <p>Course Description: <%=course.course_description %></p>
  <a href="/enroll/<%=course.course_id%>">Enroll now!</a>
  <a href="/courses/edit/<%=course.course_id%>">Edit Course Information</a>
  <hr>
  <%})%>
</div>
